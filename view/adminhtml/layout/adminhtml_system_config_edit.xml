<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="MageSquare_Base::css/config.css"/>
        <script src="MageSquare_Base::js/jscolor.min.js"/>
    </head>
    <body>
        <referenceContainer name="messages.wrapper">
            <block class="MageSquare\Base\Block\Adminhtml\Messages" name="msbase.messages" template="MageSquare_Base::messages.phtml" after="page.messages"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="MageSquare\Base\Block\Adminhtml\Notification" name="msbase.notification" template="MageSquare_Base::notification.phtml"/>
        </referenceContainer>
    </body>
</page>
